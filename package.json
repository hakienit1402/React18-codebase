{
  "name": "pulsar-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx,js,jsx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "cy": "cypress open --e2e --browser electron",
    "preview": "vite preview --port 4173",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "prettier": "prettier --check \"**/*.{tsx,ts,css,scss,js,jsx,cjs}\"",
    "prettier:fix": "prettier --write \"**/*.{tsx,ts,css,scss,js,jsx,cjs}\"",
    "prepare": "husky install",
    "types:check": "tsc --pretty --noEmit",
    "commit": "git-cz",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "depcheck": "npx depcheck --skip-missing || true",
    "prune:types": "ts-prune -I 'src/**/*.stories.tsx|src/**/*.stories.mdx'",
    "ci:check": "npm run prettier && npm run lint && npm run types:check && npm run test:run && npm run depcheck && npm run prune:types",
    "analyze": "ANALYZE=1 vite build",
    "size:report": "vite build && node -e \"const fs=require('fs');const p='dist/assets/js';let ok=true;if(!fs.existsSync(p)){process.exit(0)};fs.readdirSync(p).filter(f=>f.endsWith('.js')).forEach(f=>{const s=(fs.statSync(p+'/'+f).size/1024).toFixed(1);console.log(f,s+'kb'); if(parseFloat(s)>500){ok=false; console.error('Budget exceeded:',f,s+'kb')}}); if(!ok) process.exit(1);\"",
    "cypress:run": "vite preview --port 4173 & wait-on http://localhost:4173 && cypress run --browser chrome",
    "release": "standard-version",
    "br": "./create-branch.sh",
    "docker:dev": "docker-compose up frontend-dev --build",
    "docker:prod": "docker-compose up frontend-prod --build",
    "docker:build": "docker-compose build --no-cache",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart"
  },
